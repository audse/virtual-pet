[application]

config/name="pet-sim"
run/main_scene="res://main_menu.tscn"
config/features=PackedStringArray("4.0", "Vulkan Mobile")

[autoload]

Module="*res://utils/module.gd"
Assets="*res://utils/assets.gd"
States="*res://utils/states.gd"
Auto="*res://utils/auto.gd"

[display]

window/size/viewport_width=828
window/size/viewport_height=1792
window/size/window_width_override=2880
window/size/window_height_override=1620
window/handheld/orientation=1
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"

[filesystem]

import/blender/enabled=false

[gui]

common/default_scroll_deadzone=100
theme/custom="res://static/main.theme"
common/snap_controls_to_pixels=false

