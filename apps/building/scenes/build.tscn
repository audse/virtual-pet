script/source = "extends MeshInstance3D


func _ready() -> void:
	visible = false
	scale = Vector3(0.8, 0.0, 0.8)
	position.y = -0.25
	transparency = 1.0
	
	await get_tree().create_timer(0.6).timeout
	visible = true
	var tween := create_tween().set_ease(Tween.EASE_IN_OUT).set_trans(Tween.TRANS_SINE)
	tween.parallel().tween_property(self, \"scale:y\", 1.0, 0.3)
	tween.parallel().tween_property(self, \"position:y\", 0.0, 0.3)
	tween.parallel().tween_property(self, \"transparency\", 0.0, 0.3)
	tween.tween_property(self, \"scale\", Vector3(1.0, 1.0, 1.0), 0.5)
"

